{
	"name": "ungkirke",
	"version": "4.0.0",
	"private": true,
	"scripts": {
		"dev": "next dev -p 3000",
		"start": "next start",
		"build": "next build",
		"build:netlify": "next build && cp -r .next _next && mv _next .next/",
		"export": "npm run build && next export -o dist",
		"lint": "eslint --ext .js,.jsx .",
		"lint:fix": "eslint --fix --ext .js,.jsx . --quiet --fix --ignore-path ./.gitignore",
		"lint:format": "prettier --loglevel warn --write \"./src/**/*.{js,jsx,css,md,json}\" ",
		"lint:check": "prettier -c src",
		"lint:all": "npm run lint:format && npm run lint:fix",
		"i": "npm i --legacy-peer-deps",
		"clean": "rm -rf .next .swc _static node_modules package-lock.json",
		"clean:install": "npm run clean && npm run i",
		"clean:start": "npm run clean:install && npm run dev",
		"clean:build": "npm run clean:install && npm run build",
		"deploy": "firebase deploy --only hosting",
		"firebase": "firebase emulators:start",
		"import:db": "rm -r ./database && gsutil -m cp -r gs://worthmore-594ed.appspot.com/database .",
		"export:db": "bash ./export.bash"
	},
	"dependencies": {
		"@emotion/cache": "^11.10.5",
		"@emotion/react": "^11.10.6",
		"@emotion/server": "^11.10.0",
		"@emotion/styled": "^11.10.6",
		"@hookform/resolvers": "^2.9.11",
		"@iconify/react": "^4.1.0",
		"@mui/lab": "^5.0.0-alpha.120",
		"@mui/material": "^5.11.10",
		"encoding": "^0.1.13",
		"firebase": "^9.17.1",
		"framer-motion": "^9.1.7",
		"html-react-parser": "^3.0.9",
		"lodash": "^4.17.21",
		"next": "^13.2.1",
		"notistack": "^2.0.8",
		"nprogress": "^0.2.0",
		"numeral": "^2.0.6",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-hook-form": "^7.43.2",
		"react-lazy-load-image-component": "^1.5.6",
		"simplebar-react": "^3.2.1",
		"stylis": "^4.1.3",
		"stylis-plugin-rtl": "^2.1.1",
		"yet-another-react-lightbox": "^2.4.3",
		"yup": "^1.0.0"
	},
	"devDependencies": {
		"@babel/core": "^7.21.0",
		"@babel/eslint-parser": "^7.19.1",
		"@next/eslint-plugin-next": "^13.2.1",
		"eslint": "^8.34.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-config-prettier": "^8.6.0",
		"eslint-config-react-app": "^7.0.1",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-jsx-a11y": "^6.7.1",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"prettier": "^2.8.4"
	}
}
