{
	"name": "ungkirke",
	"version": "1.0.0",
	"scripts": {
		"i": "npm i --legacy-peer-deps",
		"rm": "rm -rf node_modules dist package-lock.json",
		"clean": "npm run rm && npm run i",
		"start": "vite",
		"build": "vite build",
		"serve": "vite preview --port 8000",
		"lint:fix": "eslint ./src --ext .jsx,.js,.ts,.tsx --quiet --fix --ignore-path ./.gitignore",
		"lint:format": "prettier --loglevel warn --write \"./src/**/*.{js,jsx,ts,tsx,css,md,json}\" ",
		"lint:check": "prettier -c src",
		"lint": "npm run lint:format && npm run lint:fix",
		"deploy": "firebase deploy --only hosting",
		"deploy:functions": "firebase deploy --only functions"
	},
	"dependencies": {
		"@emotion/cache": "latest",
		"html-react-parser": "latest",
		"react-intersection-observer": "latest",
		"@emotion/react": "latest",
		"@emotion/styled": "latest",
		"@hookform/resolvers": "latest",
		"@iconify/react": "latest",
		"@mui/lab": "latest",
		"@mui/material": "latest",
		"@reduxjs/toolkit": "latest",
		"axios": "latest",
		"change-case": "latest",
		"color": "latest",
		"date-fns": "latest",
		"firebase": "latest",
		"framer-motion": "latest",
		"history": "latest",
		"image-extensions": "latest",
		"lodash": "latest",
		"notistack": "latest",
		"nprogress": "latest",
		"numeral": "latest",
		"prop-types": "latest",
		"react": "next",
		"react-dom": "next",
		"react-helmet-async": "latest",
		"react-hook-form": "latest",
		"react-lazy-load-image-component": "latest",
		"react-lazyload": "latest",
		"react-redux": "latest",
		"react-router": "latest",
		"react-router-dom": "latest",
		"redux": "latest",
		"redux-persist": "latest",
		"rehype-raw": "latest",
		"simplebar-react": "latest",
		"smoothscroll-polyfill": "latest",
		"yup": "latest"
	},
	"devDependencies": {
		"@rollup/plugin-eslint": "latest",
		"@vitejs/plugin-react": "latest",
		"eslint": "latest",
		"eslint-config-prettier": "latest",
		"eslint-plugin-flowtype": "latest",
		"eslint-plugin-import": "latest",
		"eslint-plugin-prettier": "latest",
		"eslint-plugin-react": "latest",
		"eslint-plugin-react-hooks": "latest",
		"prettier": "latest",
		"vite": "latest",
		"vite-plugin-pwa": "latest"
	}
}
